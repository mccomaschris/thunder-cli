{
	"name": "mccomaschris/thundr-cli",
	"description": "Thundr CLI for managing Laravel and Statamic sites.",
	"type": "project",
	"autoload": {
	  "psr-4": {
		"Mccomaschris\\ThundrCli\\": "src/"
	  }
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"require": {
	  "symfony/console": "^6.0",
	  "symfony/yaml": "^6.0",
	  "symfony/process": "^6.0",
	  "laravel/prompts": "^0.3.5",
	  "php": "^8.2"
	},
	"bin": ["bin/thundr"],
	"license": "MIT",
	"require-dev": {
		"phpstan/phpstan": "^2.1",
		"laravel/pint": "^1.21",
		"pestphp/pest": "^2.36",
		"mockery/mockery": "^1.6"
	},
	"scripts": {
		"lint": "pint && phpstan analyse",
		"lint:fix": "pint",
		"lint:types": "phpstan analyse"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
